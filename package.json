{
  "name": "nextsetup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean": "npm cache clean --force && del /q /s .next && next dev",
    "devserver": "nodemon server/index.ts",
    "previewserver": "next build && tsc -p server/tsconfig.json && cross-env NODE_ENV=production node dist/index.js",
    "build": "next build",
    "buildserver": "next build && tsc -p server/tsconfig.json",
    "start": "next start",
    "startserver": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "preview": "next build && next start",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "next": "^12.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.5",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "@types/nodemon": "^1.19.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "autoprefixer": "^10.4.6",
    "eslint": "^8.19.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-validate-jsx-nesting": "^0.0.8",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.18",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.4",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|scss|md|mdx)": [
      "prettier --write"
    ]
  }
}
